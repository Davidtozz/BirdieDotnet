{
  "compilerOptions": {
    "baseUrl": "src",
    /* "paths" : {
      "components" : ["./components/*"],
      "pages" : ["pages/*"],
      "services" : ["./services/*"],
      "types" : [
        "./types/*"
      ],
      "@assets" : ["assets/*"],
    }, */
    //TODO Fix paths not being recognized
    "target": "ES6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": ["src"]
}
